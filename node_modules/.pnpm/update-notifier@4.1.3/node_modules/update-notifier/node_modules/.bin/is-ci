#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/is-ci@2.0.0/node_modules/is-ci/bin.js/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/is-ci@2.0.0/node_modules/is-ci/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/is-ci@2.0.0/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/node_modules:/mnt/d/shuil/node_modules:/mnt/d/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/update-notifier@4.1.3/node_modules/update-notifier/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/update-notifier@4.1.3/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../is-ci@2.0.0/node_modules/is-ci/bin.js" "$@"
else 
  exec node  "$basedir/../../../../../is-ci@2.0.0/node_modules/is-ci/bin.js" "$@"
fi
