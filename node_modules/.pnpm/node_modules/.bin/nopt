#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/shuil/Desktop/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/nopt@5.0.0/node_modules/nopt/bin/nopt.js/node_modules:/mnt/c/Users/shuil/Desktop/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/nopt@5.0.0/node_modules/nopt/bin/node_modules:/mnt/c/Users/shuil/Desktop/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/nopt@5.0.0/node_modules/nopt/node_modules:/mnt/c/Users/shuil/Desktop/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/nopt@5.0.0/node_modules:/mnt/c/Users/shuil/Desktop/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/node_modules:/mnt/c/Users/shuil/Desktop/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules:/mnt/c/Users/shuil/Desktop/ESTUDIO- PROGRAMACION/js-coderhouse/node_modules:/mnt/c/Users/shuil/Desktop/ESTUDIO- PROGRAMACION/node_modules:/mnt/c/Users/shuil/Desktop/node_modules:/mnt/c/Users/shuil/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/shuil/Desktop/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/node_modules/nopt/bin/nopt.js/node_modules:/mnt/c/Users/shuil/Desktop/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/node_modules/nopt/bin/node_modules:/mnt/c/Users/shuil/Desktop/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/node_modules/nopt/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nopt/bin/nopt.js" "$@"
else 
  exec node  "$basedir/../nopt/bin/nopt.js" "$@"
fi
