#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/bin/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/mkdirp@1.0.4/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/node_modules:/mnt/d/shuil/node_modules:/mnt/d/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/tar@6.1.0/node_modules/tar/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/tar@6.1.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js" "$@"
else 
  exec node  "$basedir/../../../../../mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js" "$@"
fi
