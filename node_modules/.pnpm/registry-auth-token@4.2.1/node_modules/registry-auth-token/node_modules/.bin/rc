#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/rc@1.2.8/node_modules/rc/cli.js/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/rc@1.2.8/node_modules/rc/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/rc@1.2.8/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/node_modules:/mnt/d/shuil/node_modules:/mnt/d/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/registry-auth-token@4.2.1/node_modules/registry-auth-token/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/registry-auth-token@4.2.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rc@1.2.8/node_modules/rc/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../rc@1.2.8/node_modules/rc/cli.js" "$@"
fi
