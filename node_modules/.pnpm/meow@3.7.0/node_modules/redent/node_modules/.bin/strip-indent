#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/strip-indent@1.0.1/node_modules/strip-indent/cli.js/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/strip-indent@1.0.1/node_modules/strip-indent/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/strip-indent@1.0.1/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/node_modules:/mnt/d/shuil/node_modules:/mnt/d/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/redent@1.0.0/node_modules/redent/node_modules:/mnt/d/shuil/ESTUDIO- PROGRAMACION/js-coderhouse/cromier/node_modules/.pnpm/redent@1.0.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../strip-indent@1.0.1/node_modules/strip-indent/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../strip-indent@1.0.1/node_modules/strip-indent/cli.js" "$@"
fi
